<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.5//EN" "http://struts.apache.org/dtds/struts-2.5.dtd">

<struts>
	<constant name="struts.enable.DynamicMethodInvocation" value="false" />
	<constant name="struts.devMode" value="true" />
	
	<package name="js" namespace="/js" extends="struts-default">
	
		<action name="gestion_*">
			<result>/WEB-INF/{1}/js/gerer.js</result>
		</action>
		
		<action name="bootstrap">
			<result>/WEB-INF/{1}/js/gestion_{1}.js</result>
		</action>

	</package>

</struts>